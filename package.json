{
  "name": "request-info",
  "description": "Get information about an http request.",
  "version": "0.1.1",
  "homepage": "https://github.com/doowb/request-info",
  "author": "Brian Woodward (https://github.com/doowb)",
  "repository": "doowb/request-info",
  "bugs": "https://github.com/doowb/request-info/issues",
